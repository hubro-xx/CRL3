        </binding>
        <binding name="LogServerSoap12">
          <textMessageEncoding messageVersion="Soap12" />
          <httpTransport />
        </binding>
      </customBinding>
    </bindings>
    <client>
      <endpoint address="http://127.0.0.1/UploadService.asmx" binding="basicHttpBinding" bindingConfiguration="UploadServiceSoap" contract="uploadService.UploadServiceSoap" name="UploadServiceSoap" />
      <endpoint address="http://127.0.0.1/logServer.asmx" binding="basicHttpBinding" bindingConfiguration="LogServerSoap" contract="logService.LogServerSoap" name="LogServerSoap" />
